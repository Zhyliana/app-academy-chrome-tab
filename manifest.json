{
  "manifest_version": 2,
  "name": "App Academy Chrome Tab",
  "description": "Replace default tab page in Chrome with fancy App Academy links.",
  "version": "1.0",
  "chrome_url_overrides" : {
    "newtab": "tab.html"
  },
  "permissions": ["tabs", "http://aa-progress-tracker.herokuapp.com/", "http://*.appacademy.io/"],
  "content_scripts": [
    {
      "matches": ["*://aa-progress-tracker.herokuapp.com/*", "*://*.appacademy.io/*"],
      "js": ["js/jquery-2.1.0.min.js", "js/tools.js"]
    }
  ],
  "options_page": "options.html"
}